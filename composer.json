{
  "name": "algsupport/yii2-fontawesome",
  "description": "Asset Bundle for Yii2 with Font Awesome",
  "keywords": [
    "yii",
    "font",
    "awesome",
    "asset",
    "bundle"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "minimum-stability": "stable",
  "support": {
    "issues": "https://github.com/algsupport/yii2-fontawesome/issues",
    "source": "https://github.com/algsupport/yii2-fontawesome"
  },
  "authors": [
    {
      "name": "Albert Gukasian",
      "email": "admin@algsupport.com",
      "homepage": "https://www.algsupport.com/"
    }
  ],
  "require": {
    "php": ">=8.1.0",
    "fortawesome/font-awesome": "*",
    "yiisoft/yii2": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "doctrine/instantiator": "*",
    "phpdocumentor/reflection-docblock": "~3.1.0"
  },
  "autoload": {
    "psr-4": {
      "algsupport\\yii\\fontawesome\\": ""
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
